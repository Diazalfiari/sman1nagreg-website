{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(css|js|images|favicon.ico|robots.txt)",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "src": "/build/(.*)",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false"
  },
  "build": {
    "env": {
      "COMPOSER_MIRROR_PATH_REPOS": "1"
    }
  }
}
